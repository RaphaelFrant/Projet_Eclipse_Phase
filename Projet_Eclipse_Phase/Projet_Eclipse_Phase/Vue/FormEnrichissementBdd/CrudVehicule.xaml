                </Grid>
            </GroupBox>

            <!--Boite de suppression de véhicule-->
            <GroupBox x:Name="grpboxSupprVehi" Header="Suppression de véhicules" HorizontalAlignment="Left" Height="150" Margin="43,666,0,0" VerticalAlignment="Top" Width="696" FontWeight="Bold" FontSize="16">
                <Grid x:Name="gridSupprVehi" HorizontalAlignment="Left" Height="110" Margin="0,9,-6,0" VerticalAlignment="Top" Width="690">
                    <Label x:Name="labChoixSupprVehi" Content="Choisissez un véhicule à supprimer:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontSize="14"/>
                    <ComboBox x:Name="cmbboxChoixSupprVehi" HorizontalAlignment="Left" Margin="257,10,0,0" VerticalAlignment="Top" Width="227" FontSize="14" Height="29"/>
