                    <TextBox x:Name="txtVdeCreaVehi" HorizontalAlignment="Left" Height="23" Margin="381,98,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="72" FontSize="14" FontWeight="Normal"/>
